<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="departmentalStockListAnchor" prefHeight="538.0" prefWidth="437.0" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.client.clientapplication.DepartmentalStockListController">
   <children>
      <AnchorPane fx:id="departmentalStockListTitleAnchor" layoutX="-2.0" prefHeight="40.0" prefWidth="439.0">
         <children>
            <Label layoutX="14.0" layoutY="7.0" text="Departments">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </AnchorPane>
      <AnchorPane fx:id="departmentalStockListBodyAnchor" layoutX="3.0" layoutY="43.0" prefHeight="492.0" prefWidth="427.0">
         <children>
            <Button fx:id="departmentalStockListViewDeptButton" layoutX="146.0" layoutY="447.0" mnemonicParsing="false" onAction="#fetchDepartmentStockList" prefHeight="37.0" prefWidth="134.0" text="View Department">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="departmentalStockListViewAllButton" layoutX="286.0" layoutY="447.0" mnemonicParsing="false" onAction="#fetchAllDepartmentsStockList" prefHeight="37.0" prefWidth="134.0" text="View All Depts.">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <TreeView fx:id="departmentalStockListTreeView" layoutX="6.0" layoutY="5.0" prefHeight="439.0" prefWidth="414.0" />
            <Button fx:id="departmentalStockListViewsubDeptButton" layoutX="6.0" layoutY="447.0" mnemonicParsing="false" onAction="#fetchSubDepartmentStockList" prefHeight="37.0" prefWidth="134.0" text="View Sub-Dept.">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
